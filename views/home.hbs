<h1>Student Management System</h1>
<hr>
<p><a href="/adduser" class="addbtn">Add user</a></p>
<table>
    <thead>
        <tr>
            <td>#</td>
            <td>Name</td>
            <td>Age</td>
            <td>City</td>
            <td>Edit</td>
            <td>Delete</td>
        </tr>
    </thead>
    <tbody>
        {{#each result}}
           <tr>
            <td>{{this.id}}</td>
            <td>{{this.name}}</td>
            <td>{{this.age}}</td>
            <td>{{this.city}}</td>
            <td ><a   href="/edituser/{{this.id}}" class="edit" >Edit</a></td>
            <td >  <a href="/deleteuser/{{this.id}}" class="delete" onclick="return confirm('Are you sure you want to delete this user?');">Delete</a></td>
           </tr>
        {{/each}}

    </tbody>
</table>